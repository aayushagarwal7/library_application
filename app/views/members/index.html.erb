<p id="notice"><%= params[:notice] %></p>

<h1>Members</h1>

<table>
  <thead>
  <tr>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @members.each do |member| %>
      <tr>
        <td> <%= link_to member.name, member_path(member) %> </td>
        <td><%= member.email %> </td>
      </tr>
  <% end %>
  </tbody>
</table>


